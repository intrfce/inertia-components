{
	"name": "intrfce/laravel-inertia-components",
	"description": "",
	"type": "library",
	"license": "proprietary",
	"require": {
		"php": "^8.2.0",
		"laravel/framework": "^11.0.0",
		"inertiajs/inertia-laravel": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"Intrfce\\InertiaComponents\\": "src/",
			"Intrfce\\InertiaComponents\\Tests\\": "tests/",
			"Intrfce\\InertiaComponents\\Database\\Factories\\": "database/factories/",
			"Intrfce\\InertiaComponents\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Intrfce\\InertiaComponents\\Providers\\InertiaComponentsServiceProvider"
			]
		}
	},
	"require-dev": {
		"tightenco/duster": "^3.0",
		"pestphp/pest": "^2.23",
		"orchestra/testbench": "^9.4",
		"pestphp/pest-plugin-laravel": "^2.4"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
